{
  "manifest_version":3,
  "name":"chrome Extension Demo",
  "description": "React Chrome Extension",
  "version":"1.0.0",
  "action":{
     "default_popup":"index.html",
     "default_title":"Open"
  },
  "permissions": [
    "sidePanel","activeTab"
  ],

  "side_panel": {
    "default_path": "index.html"
  }
}
