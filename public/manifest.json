{
  "manifest_version":3,
  "name":"GenAI Extension",
  "description": "React Chrome Extension",
  "version":"1.0.0",
  "action":{
     "default_popup":"index.html",
     "default_title":"Open"
  },
  "permissions": [
    "sidePanel","activeTab","tabs","scripting","*://*/*"
  ],

  "side_panel": {
    "default_path": "index.html"
  }

 
}
